   0 vi /tmp/file.txt
   1 df -h | head -3 >> /tmp/file.txt
   2 chmod g+w,u-r,o+x /tmp/file.txt
   3 ls -l /tmp/
   4 export NAME=alex
   5 export VAR=ss
   6 env
   7 echo $NAME >> /tmp/file.txt
   8 echo $VAR >> /tmp/file.txt
   9 nano /tmp/file.txt
  10 cat /tmp/file.txt
  11 history
  12 history > history.txt
